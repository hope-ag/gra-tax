{
  "$schema": "https://json.schemastore.org/swcrc",
  "env": {
  },
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": false
    },
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "module": {
    "type": "commonjs"
  },
  "sourceMaps": "inline",
  "exclude": ["logs", "__tests__"]
}